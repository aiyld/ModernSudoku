<Application x:Class="FashionSudoku.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/ColorStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/TextBlockStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/ButtonStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/Converters.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/WindowStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/ComBoxStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CommonControls;component/Themes/ListBoxStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
